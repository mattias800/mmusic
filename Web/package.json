{
  "name": "package.json",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "codegen:schema": "graphql-codegen --config codegen.schema.ts",
    "codegen:types": "graphql-codegen --config codegen.ts",
    "types": "concurrently -n schema,types -c blue,green \"bun codegen:schema --watch\" \"bun codegen:types --watch\"",
    "types:once": "concurrently -n schema,types -c blue,green \"bun codegen:schema\" \"bun codegen:types\"",
    "prettier": "prettier --no-config --write \"./src/**/*.{ts,tsx,css,graphql}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.12",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.5",
    "@types/lodash-es": "^4.17.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.507.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@parcel/watcher": "^2.5.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@urql/exchange-graphcache": "^7.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "urql": "^4.2.2",
    "vite": "^6.3.1",
    "vite-plugin-checker": "^0.9.3",
    "vitest": "^3.1.4"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
